// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/*.map": true,
        "**/node_modules": false,
        "**/*.js": { "when": "$(basename).ts" }
    },
    "angularKarmaTestExplorer.defaultSocketConnectionPort": 9998,
    "angularKarmaTestExplorer.projectType": "Angular",
    "angularKarmaTestExplorer.projectRootPath": "/"
}